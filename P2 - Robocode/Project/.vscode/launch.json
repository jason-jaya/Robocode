{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Robocode",
            "request": "launch",
            "mainClass": "robocode.Robocode",
            "vmArgs": ["-Xmx512M", "-Ddebug","-cp", "libs/*", "-Djava.security.manager=allow", "-XX:+IgnoreUnrecognizedVMOptions", "--add-opens=java.base/sun.net.www.protocol.jar=ALL-UNNAMED", "--add-opens=java.base/java.lang.reflect=ALL-UNNAMED", "--add-opens=java.desktop/javax.swing.text=ALL-UNNAMED", "--add-opens=java.desktop/sun.awt=ALL-UNNAMED"],
            "cwd": "${workspaceFolder}/../env",
//            "preLaunchTask": "Build Workspace"
        },
    ]
}